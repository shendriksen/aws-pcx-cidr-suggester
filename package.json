{
  "name": "aws-pcx-cidr-suggester",
  "version": "0.0.1",
  "description": "Suggest an available cidr range for a peering connection",
  "main": "lib/index.js",
  "repository": "git@github.com:shendriksen/aws-pcx-cidr-suggester.git",
  "author": "Sam Hendriksen <sam.hendriksen@ecs.co.uk>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.305.0",
    "cidr-matcher": "^1.0.5",
    "cidr-overlap": "^0.0.4",
    "ip-cidr": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "lint-staged": "^7.1.0",
    "mocha": "^5.1.0",
    "nock": "^9.2.6",
    "nyc": "^11.7.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "build": "babel src -d lib --copy-files",
    "lint": "./node_modules/.bin/eslint src/* --fix",
    "lint-staged": "lint-staged",
    "test": "nyc mocha -r chai/register-should --require babel-core/register './src/**/*.test.js'",
    "test:unit": "mocha -r chai/register-should --require babel-core/register './src/**/*.test.js'",
    "prepare": "npm run build",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.test.js"
    ],
    "sourceMap": true,
    "instrument": true,
    "require": [
      "babel-register"
    ],
    "all": true,
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint-staged"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "./node_modules/.bin/eslint src/* --fix",
      "git add"
    ]
  }
}
